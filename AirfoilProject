xlswrite(filename,Fchord, 1, 'H259' )
power = ((D_Air*Vac)/(Eprop/100))/550
xlswrite(filename,power, 1, 'H260' )
d = 1-(DEN/0.00237691)
powersea = power*((DEN/0.00237691)-(d/7.55))^-1
xlswrite(filename,powersea, 1, 'H261' )
